    static int frameCount = 0;
    if (frameCount++ % 300 == 0) // Log every 5 seconds at 60fps
    {
        DEBUG_LOG("DEBUG_MODE indicator rendered in upper right corner at (" << x << ", " << y << ")");
        DEBUG_LOG("Debug white cube rendered at origin (0, 0, 0)");
    }
