    {
        auto it = parameters.find(name);
        if (it == parameters.end())
        {
            return defaultValue;
        }

        std::istringstream iss(it->second);
        Color result = defaultValue;
        iss >> result.r >> result.g >> result.b >> result.a;
        return result;
